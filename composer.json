{
  "name": "sanetchek/emptytheme",
  "type": "wordpress-theme",
  "description": "emptytheme starter theme with localization support.",
  "keywords": [
    "WordPress",
    "Theme",
    "Localization"
  ],
  "homepage": "https://github.com/Sanetchek/empty-theme",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Contributors",
      "homepage": "https://github.com/Sanetchek/empty-theme/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "wp-cli/i18n-command": "^2.2.5"
  },
  "scripts": {
    "make-pot": "vendor/bin/wp i18n make-pot . languages/emptytheme.pot",
    "make-content": "node generateContent.js",
    "create-gulp": "@generate-gulpfile",
    "create-package": "@generate-package",
    "create-script": "@generate-script",
    "generate-script": [
      "@php -r \"file_put_contents('generateContent.js', base64_decode(''));\""
    ],
    "generate-gulpfile": [
      "@php -r \"file_put_contents('gulpfile.js', base64_decode(''));\""
    ],
    "generate-package": [
      "@php -r \"file_put_contents('package.json', base64_decode(''));\""
    ],
    "clean-prod": [
      "@php -r \"foreach (['generateContent.js', 'project_content.txt'] as $file) { if (file_exists($file)) unlink($file); }\""
    ],
    "dev": [
      "composer install",
      "npm install"
    ],
    "prod": [
      "npm run build",
      "composer make-pot",
      "@clean-prod",
      "composer install --no-dev --optimize-autoloader",
      "rm -rf node_modules package-lock.json vendor composer.lock"
    ]
  },
  "support": {
    "issues": "https://github.com/Sanetchek/empty-theme/issues",
    "source": "https://github.com/Sanetchek/empty-theme"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": false
    }
  }
}
